<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.needham.thomas.medicare"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".root.Activities.MainActivity"
            android:theme="@android:style/Theme.Holo.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".root.Activities.LoginActivity">
        </activity>
        <activity android:name=".root.Activities.RegisterActivity">
        </activity>
        <activity android:name=".root.Activities.HomeActivity">
        </activity>
        <activity android:name=".root.Activities.NewRecordActivity">
        </activity>
    </application>

</manifest>
